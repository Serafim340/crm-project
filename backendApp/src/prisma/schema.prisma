generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id String @id @default(uuid())
  login String @unique
  name String
  surname String
  password String
  createdAt DateTime @default(now())
}
model Location {
  id        String   @id @default(uuid())
  name      String   @unique
  text      String
  address   String
  createdAt DateTime @default(now())
}

{
  "name": "@WebApp/frontendApp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "types": "tsc --noEmit --project ./tsconfig.json && tsc --noEmit --project ./tsconfig.node.json",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts,.tsx .",
    "stylelint": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache \"src/**/*.scss\"",
    "stylecheck": "sass --update -q ./src:./node_modules/.cache/.stylecheckcache",
    "prettify": "prettier --log-level warn --cache --write ."
  },
  "dependencies": {
    "@WebApp/backendApp": "workspace:*",
    "@tanstack/react-query": "^4.35.3",
    "@trpc/client": "^10.45.2",
    "@trpc/react-query": "^10.45.2",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "formik": "^2.4.6",
    "formik-validator-zod": "^2.0.1",
    "include-media": "^2.0.0",
    "js-cookie": "^3.0.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.1.1",
    "reset-css": "^5.0.2",
    "sass": "^1.66.1",
    "superjson": "^1.13.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "globals": "^15.13.0",
    "prettier": "^3.4.2",
    "stylelint": "^15.10.3",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "vite": "^4.4.9"
  }
}
